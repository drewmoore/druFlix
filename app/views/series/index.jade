extends ../shared/template

block content

  .row
    .small-12.columns
      h1.series-index-head #{title}
  .row
    .small-12.columns
      each series in serieses
        - var id = series._id.toString();
        .series-container
          a(href='/series/show/' + id)
            img.series-cover(src=series.cover)
          p.series-info #{series.title}


